{
  "name": "cpsm",
  "version": "0.0.0",
  "description": "Competitive Programming Solutions Manager",
  "main": "(none)",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.md": [
      "markdown-toc -i",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.py": [
      "yapf -i",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btjanaka/cpsm.git"
  },
  "keywords": [
    "hackerrank",
    "kattis",
    "leetcode",
    "uva",
    "python",
    "jinja",
    "competitive-programming",
    "tool"
  ],
  "author": "btjanaka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btjanaka/cpsm/issues"
  },
  "homepage": "https://github.com/btjanaka/cpsm#readme",
  "devDependencies": {
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "markdown-toc": "1.2.0",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
